{
  "name": "parafoil",
  "version": "0.0.4",
  "description": "Yet another slider for React, but the last you'll ever need.",
  "author": "Josh Perez <josh.perez@airbnb.com>",
  "license": "MIT",
  "repository": "Vlad-Zhukov/parafoil",
  "bugs": {
    "url": "https://github.com/Vlad-Zhukov/parafoil/issues"
  },
  "main": "./dist/parafoil.cjs.js",
  "module": "./dist/parafoil.esm.js",
  "scripts": {
    "build": "rimraf dist/ && cross-env NODE_ENV=production rollup -c",
    "test": "jest",
    "lint": "eslint \"**/*.js\"",
    "prettier": "prettier-eslint \"**/*.js\" --write",
    "prepublishOnly": "yarn build && yarn lint && yarn test"
  },
  "dependencies": {
    "prop-types": "^15.6.1"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.46",
    "@babel/preset-env": "^7.0.0-beta.46",
    "@babel/preset-react": "^7.0.0-beta.46",
    "@babel/preset-stage-3": "^7.0.0-beta.46",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^8.2.3",
    "babel-jest": "^22.4.3",
    "cross-env": "^5.1.4",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "jest": "^22.4.3",
    "prettier-eslint-cli": "^4.7.1",
    "raf": "^3.4.0",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "rimraf": "^2.6.2",
    "rollup": "^0.58.2",
    "rollup-plugin-babel": "^4.0.0-beta.4"
  },
  "peerDependencies": {
    "react": ">=15"
  },
  "keywords": [
    "react",
    "slider",
    "react-slider"
  ],
  "files": [
    "src",
    "dist"
  ],
  "jest": {
    "roots": [
      "<rootDir>/__tests__/"
    ],
    "transform": {
      ".js$": "babel-jest"
    }
  }
}
